@model WebDeshiHutBazar.PortfolioInfoViewModel
@{
    ViewBag.Title = Common.Language.LanguageDefault.ViewBagTitleAllItemMarket;
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ExcludeHeader = false;
}
<div id="layoutBodyContainer" class="container">
    <div class="row">
        @foreach (var row in Model.ListUserAll)
        {
            <div class="col-md-4">
                @Html.Partial("../../Areas/LetItGo/Views/Portfolio/_UserItemDesktop", row)
            </div>
        }
    </div>
</div>
@section scriptsapp {
    <script type="text/javascript">
        PAGE_NAME = '@Model.PageName';
        var tab = "";
    </script>
    
    <script src="~/Scripts/js/LetItGo/naimul.letitgo.market.js" type="text/javascript"></script>
    <script src="~/Scripts/js/UserSession/naimul.letitgo.user.session.js" type="text/javascript"></script>
}



